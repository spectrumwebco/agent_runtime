  "scripts": {
    "dev": "npm run make-i18n && cross-env VITE_MOCK_API=false webpack serve --mode development --config webpack.config.mjs",
    "dev:mock": "npm run make-i18n && cross-env VITE_MOCK_API=true VITE_MOCK_SAAS=false webpack serve --mode development --config webpack.config.mjs",
    "dev:mock:saas": "npm run make-i18n && cross-env VITE_MOCK_API=true VITE_MOCK_SAAS=true webpack serve --mode development --config webpack.config.mjs",
    "build": "npm run make-i18n && webpack --mode production --config webpack.config.mjs",
    "build:electron": "echo 'Electron build temporarily disabled'",
    "build:lynx": "echo 'Lynx build temporarily disabled'",
    "start": "npx sirv-cli dist/ --single",
    "start:electron": "electron dist/electron/main.js",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "test:coverage": "npm run make-i18n && vitest run --coverage",
    "make-i18n": "node scripts/make-i18n-translations.cjs",
    "prelint": "npm run make-i18n",
    "lint": "npm run typecheck && eslint src --ext .ts,.tsx,.js && prettier --check src/**/*.{ts,tsx}",
    "lint:fix": "eslint src --ext .ts,.tsx,.js --fix && prettier --write src/**/*.{ts,tsx}",
    "prepare": "cd .. && husky frontend/.husky",
    "typecheck": "tsc",
    "check-unlocalized-strings": "node scripts/check-unlocalized-strings.cjs",
    "setup-shadcn": "node scripts/setup-shadcn.js"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
